def find_peaks(data, sig=2.0, binsize=100):
    # above standard deviation
    peak_inds = []

    i = 0
    while i < len(data):
        bin_begin = max(0, (i - binsize/2))
        bin_end = min(len(data),(i+binsize/2))
        the_bin = data[bin_begin:bin_end]

        std = np.nanstd(np.sort(the_bin)[1:])
        median = np.median(the_bin)

        upperbound = median + sig*std

        if data[i] > upperbound:
            peak_inds.append(i)

        i = i + 1

    return peak_inds

def find_peaks2(data, thresh, binsize=100, mfn=21):
    # detect_peaks
    data -= mf(data, mfn)
    peak_inds = []

    i = 0
    while i < len(data):
        bin_begin = max(0, (i - binsize/2))
        bin_end = min(len(data),(i+binsize/2))
        the_bin = np.asarray(data[bin_begin:bin_end])

        # std = np.nanstd(np.sort(the_bin)[1:])
        median = np.median(the_bin)

        the_bin += min(data)
        peak_inds += list(detect_peaks.detect_peaks(ar_i, mph=None, mpd=5, threshold=thresh))

        i = i + 1

    return peak_inds

def find_peaks3(data, binsize=150, nthresh=0.3, hthresh=1.2):
    # ?
    peak_inds = []
    all_inds = []
    meds = []

    i = 0
    while i < len(data):
        bin_begin = max(0, (i - binsize / 2))
        bin_end = min(len(data), (i + binsize / 2))
        the_bin = np.asarray(data[bin_begin:bin_end])

        the_bin += min(the_bin)

        # std = np.nanstd(np.sort(the_bin)[1:])
        median = np.median(the_bin)
        meds.append(median)

        # upperbound = median + sig * std

        all_inds.append(list(the_bin).index(max(the_bin)) + bin_begin)

        i = i + 1

    unique_inds = sorted(list(set(all_inds)))
    counts, heights = [], []
    for ind in unique_inds:
        h = data[ind] / meds[ind]
        c = all_inds.count(ind)
        if c > binsize*nthresh and h > hthresh:
            peak_inds.append(ind)

        counts.append(c*100)
        heights.append(h)

    plt.plot(unique_inds, heights)
    plt.plot(unique_inds, counts)
    plt.show()

    return peak_inds

